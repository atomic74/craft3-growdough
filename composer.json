{
    "name": "tungsten/grow-dough",
    "description": "This plugin allows collecting donation designations in a Donations List that works similar to a shopping cart.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "growdough"
    ],
    "support": {
        "docs": "https://github.com/atomic74/craft3-growdough/blob/master/README.md",
        "issues": "https://github.com/atomic74/craft3-growdough/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tungsten Creative Group",
            "homepage": "https://www.atomic74.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "tungsten\\growdough\\": "src/"
        }
    },
    "extra": {
        "name": "GrowDough",
        "handle": "grow-dough",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/atomic74/grow-dough/master/CHANGELOG.md",
        "components": {
            "service": "tungsten\\growdough\\services\\Service",
            "helpersService": "tungsten\\growdough\\services\\HelpersService"
        },
        "class": "tungsten\\growdough\\GrowDough"
    }
}
